{
    "name": "gigasavvy/liteview-api",
    "description": "A PHP wrapper around the Liteview REST API",
    "keywordfs": ["liteview", "api", "fulfillment", "ecommerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Rapaport",
            "email": "rapaport.sam7@gmail.com"
        }
    ],
    "autoload": {
